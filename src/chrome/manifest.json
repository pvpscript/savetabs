{
	"name": "Save tabs",
	"version": "0.0.1",
	"description": "Save opened urls for every open tab and every opened window",
	"manifest_version": 2,
	"permissions": ["tabs", "downloads"],

	"icons": {
		"128": "icons/icon128.png",
		"96": "icons/icon96.png",
		"64": "icons/icon64.png",
		"32": "icons/icon32.png",
		"16": "icons/icon16.png"
	},

	"background": {
		"scripts": ["background_scripts/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "Save tabs",
		"default_popup": "popup/popup.html"
	}
}
